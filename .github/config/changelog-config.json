{
  "mode": "COMMIT",
  "template": "#{{CHANGELOG}}",
  "categories": [
    {
      "title": "## âœ¨ Features",
      "labels": ["feat", "feature"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## ğŸ”§ Others",
      "labels": ["chore", "docs", "refactor", "perf", "ci"]
    }
  ],
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
      "on_property": "title",
      "target": "$1"
    }
  ]
}
